{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "plugins": ["eslint-plugin-prettier/recommended"],
  "rules": {
    "prettier/prettier": "warn",
    "react/react-in-jsx-scope": "off",
    "no-console": "warn",
    "no-unused-vars": "warn",
    "no-debugger": "warn",
    "react/prop-types": "off"
  }
}
